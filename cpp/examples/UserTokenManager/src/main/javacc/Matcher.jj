options {
	BUILD_PARSER=true;
	BUILD_TOKEN_MANAGER=false;
	USER_TOKEN_MANAGER=true;
	NAMESPACE="Basic";
}	
PARSER_BEGIN(Matcher)
static const int simple = 4;
PARSER_END(Matcher)


void Input() :
{}
{
  MatchedBraces() (<NL> | <LF>)* <EOF>
}

/** Brace matching production. */
void MatchedBraces() :
{}
{
  <LBRACE> [ MatchedBraces() ] <RBRACE>
}
